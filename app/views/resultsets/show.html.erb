<section>
  <div class="col-sm-12">
    <p>
      <strong>Name:</strong>
      <%= @resultset.name %>
    </p>
  </div>
  <div class="col-sm-6">
    <strong>Index:</strong>
    <%= rs_link_to_index(@resultset) %>
    <p>
      <%= json_editor(@resultset.index_definition, id: 'index-definition-viewer', readonly: true) %>
    </p>
  </div>
  <div class="col-sm-6">
    <strong>Query</strong>
    <p>
      <%= json_editor(@resultset.query, id: 'query-viewer', readonly: true) %>
    </p>
  </div>
  <div class="col-sm-12">
    <strong>Result</strong>
    <p>
      <%= json_editor(@resultset.result, id: 'result-viewer', readonly: true) %>
    </p>
  </div>
  <div class="col-sm-12">
    <strong>Dataset:</strong>
    <%= rs_link_to_dataset(@resultset) %>
    <p>
      <%= ndjson_editor(@resultset.dataset_preview, readonly: true) %>
    </p>
  </div>
</section>
<section>
  <div class="col-sm-12">
    <%= link_to 'Edit Name', edit_resultset_path(@resultset) %> |
    <%= link_to 'Destroy', @resultset, method: :delete, data: { confirm: "Are you sure you want to destroy '#{@resultset.name}'?" } %> |
    <%= link_to 'List', resultsets_path %>
  </div>
</section>
